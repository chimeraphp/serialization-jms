{
    "name": "lcobucci/chimera-serialization-jms",
    "description": "Provides message creator and response converter that uses jms/serializer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "http-interop/http-middleware": "^0.4",
        "jms/serializer": "^1.9",
        "lcobucci/chimera-foundation": "1.0.0-alpha1@alpha",
        "lcobucci/chimera-routing": "1.0.0-alpha1@alpha",
        "lcobucci/chimera-routing-expressive": "1.0.0-alpha1@alpha",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\Chimera\\Serialization\\Jms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\Chimera\\Serialization\\Jms\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
