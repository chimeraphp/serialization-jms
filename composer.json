{
    "name": "chimera/serialization-jms",
    "description": "Adapter for jms/serializer which allows you to create messages to be handled",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "chimera/foundation": "^0.4@dev",
        "jms/serializer": "^3.10"
    },
    "require-dev": {
        "doctrine/annotations": "^1.11",
        "infection/infection": "^0.20",
        "lcobucci/coding-standard": "^6.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^9.4"
    },
    "provide": {
        "chimera/serialization-implementation": "0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Chimera\\MessageCreator\\JmsSerializer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chimera\\MessageCreator\\JmsSerializer\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
