{
    "name": "lcobucci/chimera-serialization-jms",
    "description": "Adapter for jms/serializer which allows you to create messages to be handled",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "jms/serializer": "^1.11",
        "lcobucci/chimera-foundation": "^1.0@rc"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\Chimera\\MessageCreator\\JmsSerializer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\Chimera\\MessageCreator\\JmsSerializer\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
