{
    "name": "chimera/serialization-jms",
    "description": "Adapter for jms/serializer which allows you to create messages to be handled",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "jms/serializer": "^3.0",
        "chimera/foundation": "^0.3"
    },
    "require-dev": {
        "infection/infection": "^0.13 || ^0.14",
        "lcobucci/coding-standard": "^3.0",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-deprecation-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Chimera\\MessageCreator\\JmsSerializer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chimera\\MessageCreator\\JmsSerializer\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
